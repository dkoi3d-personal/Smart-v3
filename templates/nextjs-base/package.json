{
  "name": "nextjs-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "verify": "./node_modules/.bin/tsc --noEmit",
    "clean:build": "node scripts/clean-build.cjs",
